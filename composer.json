{
  "name": "gpexinc/gpex_bittrex",
  "description": "Bittrex API wrapper for Laravel",
  "tags": ["bittrex", "api", "laravel"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabe Salgado",
      "email": "gabe@tourbrat.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-curl": "*",
    "laravel/framework": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "gpexinc\\Bittrex\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "gpexinc\\Bittrex\\BittrexServiceProvider"
      ],
      "aliases": {
        "Bittrex": "gpexinc\\Bittrex\\Bittrex"
      }
    }
  }
}
